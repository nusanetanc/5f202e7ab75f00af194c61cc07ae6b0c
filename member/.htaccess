RewriteRule ^package-add$ ?hal=package-add
RewriteRule ^package-list$ ?hal=package-list
RewriteRule ^payment$ ?hal=payment
RewriteRule ^payment-confrim$ ?hal=paymentconfrim
RewriteRule ^registration-add$ ?hal=registration-add
RewriteRule ^report-jobs/([0-9A-Za-z]+)/? ?hal=report-jobs&id=$1
