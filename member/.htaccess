RewriteRule ^customer-profile/([0-9A-Za-z]+)/? ?hal=customer-profile&id_cust=$1
RewriteRule ^helpdesk-add$ ?hal=helpdesk-add
RewriteRule ^helpdesk-list$ ?hal=helpdesk-list
