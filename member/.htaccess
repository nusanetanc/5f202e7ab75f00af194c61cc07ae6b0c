RewriteRule ^support-add$ ?hal=support-add
RewriteRule ^support-list$ ?hal=support-list
RewriteRule ^customer/([0-9A-Za-z]+)/? ?hal=customer&status=$1
RewriteRule ^customer$ ?hal=customer
RewriteRule ^add-on$ ?hal=add-on

RewriteRule ^support$ ?hal=support
RewriteRule ^user-add$ ?hal=user-add
RewriteRule ^user-list$ ?hal=user-list
RewriteRule ^verification-payment/([0-9A-Za-z]+)/? ?hal=verification-payment&id_cust=$1
RewriteRule ^verification-registrasi$ ?hal=verification-registrasi
RewriteRule ^write-maintenance-info$ ?hal=write-maintenance-info
RewriteRule ^logout$ ?hal=logout

</IfModule>
